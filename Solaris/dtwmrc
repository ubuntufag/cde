###
#
# Root Menu Description
#
###

Menu DtRootMenu
{
    "Workspace Menu"			f.title
    
# Lines below are generated by dtaction GenerateRootMenu.  Do not edit them.
# vvv dtaction GenerateRootMenu vvv
    "Terminal"		f.action Terminal
    "no-label"		f.separator
    "Applications"		f.menu ApplicationsMenu
    "Cards"		f.menu CardsMenu
    "Files"		f.menu FilesMenu
    "Folders"		f.menu FoldersMenu
    "Help"		f.menu HelpMenu
    "Hosts"		f.menu HostsMenu
    "Links"		f.menu LinksMenu
    "Mail"		f.menu MailMenu
    "Tools"		f.menu ToolsMenu
# ^^^ dtaction GenerateRootMenu ^^^
# Lines above are generated by dtaction GenerateRootMenu.  Do not edit them.
     no-label			f.separator
    "Windows"			f.menu WindowsMenu
     no-label			f.separator
    "Add Item to Menu..."	f.action AddItemToMenu
    "Customize Menu..."		f.action CustomizeWorkspaceMenu
     no-label			f.separator
    "Lock Display"		f.action LockDisplay

     "Log out..."			f.action ExitSession
}

Menu WindowsMenu
{
    "Windows"				f.title
    "Shuffle Up"			f.circle_up
    "Shuffle Down"			f.circle_down
    "Refresh"				f.refresh
    "Clean Up Icons"			f.pack_icons
     no-label				f.separator
    "Minimize/Restore Front Panel"	f.toggle_frontpanel
    "Restart Workspace Manager..."	f.restart
    "Update Workspace Menu"		f.action UpdateWorkspaceMenu
     no-label				f.separator
    "Disable Access Control"		f.action Xhost+
    "Enable Access Control"		f.action Xhost-
     no-label				f.separator
    "Kill Window..."			f.exec /usr/openwin/bin/xkill
}

